{
  "name": "Spoiler blocker",
  "version": "0.0",
  "manifest_version": 2,
  "content_scripts": [
   {
     "matches": ["*://*.facebook.com/*"],
     "js": [
      "jquery-3.4.1.js",
      "script.js",
      "popup.js"
     ]
   }
 ],
  "options_page": "options.html",
  "browser_action": {
  	"default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}